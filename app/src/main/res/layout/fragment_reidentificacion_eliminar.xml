<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context="com.mifincaapp.mifincaapp.Reidentificacion_eliminar">

    <RelativeLayout
        android:id="@+id/content_control_peso_registrar"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:paddingBottom="@dimen/activity_vertical_margin"
        android:paddingLeft="@dimen/activity_horizontal_margin"
        android:paddingRight="@dimen/activity_horizontal_margin"
        android:paddingTop="@dimen/activity_vertical_margin"
        app:layout_behavior="@string/appbar_scrolling_view_behavior"
        tools:context="com.mifincaapp.mifincaapp.ControlPeso_registrar"
        tools:showIn="@layout/activity_control_peso_registrar">

        <TextView
            android:textSize="20dp"
            android:text="Reidentificación"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView2"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="120dp"/>

        <TextView
            android:text="Número:"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView3"
            android:textSize="18dp"
            android:layout_marginTop="60dp"
            android:layout_marginLeft="20dp"/>

        <TextView
            android:text="Fecha"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_alignLeft="@+id/textView3"
            android:layout_alignStart="@+id/textView3"
            android:id="@+id/textView5"
            android:textSize="18dp"
            android:layout_marginTop="100dp" />

        <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="date"
            android:ems="10"
            android:layout_below="@+id/textView3"
            android:layout_alignLeft="@+id/textID"
            android:layout_alignStart="@+id/textID"
            android:text="20/11/16"
            android:textAlignment="center"
            android:id="@+id/txtFechaRegistrarPeso"
            android:layout_marginTop="10dp"/>

        <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="number"
            android:textAlignment="center"
            android:layout_marginTop="10dp"
            android:ems="10"
            android:layout_below="@+id/textView5"
            android:layout_alignLeft="@+id/txtFechaRegistrarPeso"
            android:layout_alignStart="@+id/txtFechaRegistrarPeso"
            android:id="@+id/txtArete"
            android:text="29" />

        <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="number"
            android:text="5"
            android:textAlignment="center"
            android:ems="8"
            android:id="@+id/textID"
            android:layout_alignBaseline="@+id/textView3"
            android:layout_alignBottom="@+id/textView3"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true" />

        <TextView
            android:text="Num. arete nuevo:"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_alignLeft="@+id/textView10"
            android:layout_alignStart="@+id/textView10"
            android:id="@+id/textView12"
            android:textSize="18dp"
            android:layout_marginTop="180dp"/>

        <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="number"
            android:text="368"
            android:textAlignment="center"
            android:ems="10"
            android:id="@+id/editText5"
            android:layout_below="@+id/txtArete"
            android:layout_alignLeft="@+id/txtArete"
            android:layout_alignStart="@+id/txtArete" />

        <TextView
            android:text="Motivo del cambio:"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_alignLeft="@+id/textView12"
            android:layout_alignStart="@+id/textView12"
            android:id="@+id/textView13"
            android:textSize="18dp"
            android:layout_marginTop="220dp"/>

        <MultiAutoCompleteTextView
            android:text="P"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/multiAutoCompleteTextView"
            tools:text="Perdida arete"
            android:layout_alignBaseline="@+id/textView13"
            android:layout_alignBottom="@+id/textView13"
            android:layout_toRightOf="@+id/textView10"
            android:layout_marginLeft="19dp"
            android:layout_marginStart="19dp"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            android:textAlignment="center"/>

        <Button
            android:text="Eliminar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/button2"
            android:layout_marginTop="350dp"
            android:layout_marginLeft="70dp"/>

        <Button
            android:text="Cancelar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/button3"
            android:layout_marginTop="350dp"
            android:layout_marginLeft="210dp"/>

        <TextView
            android:text="Núm. arete anterior:"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView10"
            android:textSize="18dp"
            android:layout_alignBaseline="@+id/txtArete"
            android:layout_alignBottom="@+id/txtArete"
            android:layout_alignLeft="@+id/textView5"
            android:layout_alignStart="@+id/textView5" />

    </RelativeLayout>

</FrameLayout>
